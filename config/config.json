{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "migration-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/migration/MigrationWebPart.js",
          "manifest": "./src/webparts/migration/MigrationWebPart.manifest.json"
        }
      ]
    },

    "my-published-viewer-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/myPublishedViewer/MyPublishedViewerWebPart.js",
          "manifest": "./src/webparts/myPublishedViewer/MyPublishedViewerWebPart.manifest.json"
        }
      ]
    }

  },
  "externals": {},
  "localizedResources": {
    "MigrationWebPartStrings": "lib/webparts/migration/loc/{locale}.js",
    "MyPublishedViewerWebPartStrings": "lib/webparts/myPublishedViewer/loc/{locale}.js"
  }
}

